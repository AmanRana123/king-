#include<iostream>
using namespace std;
int main(){
    int n;
	cout<<"enetr n"<<endl;
	cin>>n;
	int i=1;
	
	while(i<=n){
		int spaces=1;
		while(spaces<=n-i){
			cout<<" ";
			spaces =spaces+1;
		}
		int j=1;
		int k=i;
	while(j<=i){
		cout<<k;
		j=j+1;
		k=k+1;
	    
	}
	cout<<endl;
	i=i+1;
	
		
	}
}
